      <ul id="navbar-secondary" class="links-list">
        <li class="navbar-secondary-link">
          <%= link_to post_path do %>
            <i class="link-icon material-icons">add</i>Add Project
          <% end %>
        </li>
        <li class="navbar-secondary-link">
          <%= link_to '#' do %>
            <i class="link-icon material-icons">account_box</i>Profile
          <% end %>
